{
	"manifest_version": 3,
	"name": "Tab Defender",
	"description": "A game to defend your Ram memory from the evil tab monsters!",
	"version": "1.0.1",
	"action": {
	  "default_popup": "index.html",
	  "default_icon": "images/favicon-512x512.png"
	},
	"icons": {
		"16": "images/favicon-16x16.png",
		"32": "images/favicon-32x32.png",
		"48": "images/favicon-48x48.png",
		"128": "images/favicon-128x128.png"
	},
	"permissions": [
		"tabs"
	]
  }